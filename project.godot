; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Surfsup SDK"
config/description="SurfsUp Map Making SDK"
config/quit_on_go_back=false
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="uid://ysqanrf6aw6x"

[debug]

gdscript/warnings/untyped_declaration=1

[editor_plugins]

enabled=PackedStringArray("res://addons/SurfsUpSDK/plugin.cfg", "res://addons/godotfgd/plugin.cfg", "res://addons/godotvmf/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "red",
"res://Levels/": "orange",
"res://Scenes/": "yellow",
"res://Scripts/": "green",
"res://Shaders/": "blue",
"res://docs/": "gray"
}

[surfs_up]

maps_directory="/home/mark/Godot/Source/surfs-up/Maps"
