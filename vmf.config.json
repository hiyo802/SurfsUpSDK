{
	"gameinfo_path": "res://",
	"vtfcmd": "res://tools/VTFCmd.exe",

	"import": {
		"scale": 0.02,
		"generate_collision": true,
		"generate_lightmap_uv2": true,
		"lightmap_texel_size": 0.4,
		"instances_folder": "res://vmf/instances",
		"entities_folder": "res://vmf/entities",
		"geometry_folder": "res://vmf/geometry",
		"use_navigation_mesh": false,
	},

	"models": {
		"import": true,
		"lightmap_texel_size": 0.4,
		"target_folder": "res://vmf/models"
	},

	"materials": {
		"import_mode": 1,
		"ignore": [
			"TOOLS/TOOLSNODRAW",
			"TOOLS/TOOLSSKYBOX",
			"TOOLS/TOOLSINVISIBLE"
		],
		"fallback_material": null,
		"default_texture_size": 512,
		"target_folder": "res://vmf/materials"
	}
}
